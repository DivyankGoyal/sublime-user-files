<snippet>
	<content><![CDATA[
void rotate(vector<ll>::iterator it1,vector<ll>::iterator it2,ll k)
{
    k = -k;
    ll n = it2-it1;
    k = k%n;
    if(k<0)
        k = k + n;
    rotate(it1,it1+k,it2);
}
]]></content>
	<!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
	<tabTrigger>trotate</tabTrigger>
	<!-- Optional: Set a scope to limit where the snippet will trigger -->
	<scope>source.c++</scope>
</snippet>
